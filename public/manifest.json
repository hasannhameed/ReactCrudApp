{
    "manifest_version": 3,
    "name": "React To-Do Extension",
    "version": "1.0",
    "description": "A simple To-Do List as a Chrome Extension",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    }
  }
  